
// Je begint met een waarde in seconden
float seconden = 1234567.0;  // Vervang dit door jouw waarde

// Bereken het aantal uren, dagen en jaren
float uren = seconden / 3600.0;  // Er zijn 3600 seconden in een uur
float dagen = uren / 24.0;      // Er zijn 24 uren in een dag
float jaren = dagen / 365.0;    // Dit is een vereenvoudigde benadering van een jaar

  // Geef de resultaten weer

print("Seconden: ");
println(seconden);
print("Uren: ");
println(uren);
print("Dagen: ");
println(dagen);
print("Jaren: ");
println(jaren);
