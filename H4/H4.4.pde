
//afronden  doen we met de round methode.

float getal = 10.2;
long afgerondGetal = Math.round(getal);
 System.out.println(afgerondGetal);

//ceil methode. Deze rond af naar het eerstvolgende hele getal.

float getalA = 2.3;
long cMethode = (long) Math.ceil(getalA);
System.out.println(cMethode);

//floor methode. Deze rond af naar het vorige hele getal.

float getalB = 2.3;
long bMethode = (long) Math.floor(getalB);
System.out.println(bMethode);
